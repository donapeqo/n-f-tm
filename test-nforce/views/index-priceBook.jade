extends ../node_modules/jade-bootstrap/layouts/navbar-fixed-force

append body

  +navbar("tmusaApp","navbar", "inverse")
    +nav_item("/") Home
    +nav_item("/accounts") Accounts
    +nav_item("/contacts") Contacts
    +nav_item("#") Opportunities
    +nav_item("#") Price Book

  .container
    div
      p
        'Test' + records._fields
  
  .container
    div(style='padding-bottom:10px;')

    div
      form.form-signin(action="/create-account", method="get")
      table.table.table-striped.table-hover.span12
        thead
          tr 
            th Name 
            th Account
            th Account Owner
            th Account Created By
            th Cable Route
            th Date
            th MRC
            th OTC
        tbody
          - each result, index in records
              tr
                td= index + result._fields
                


 

       

